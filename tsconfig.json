{
  "compilerOptions": {
    "allowUnreachableCode": true,
    "allowUnusedLabels": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "lib": ["ES6", "DOM"],
    "module": "CommonJS",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitReturns": false,
    "sourceMap": true,
    "strict": false,
    "target": "ES6",
    "removeComments": false,
  }
}
